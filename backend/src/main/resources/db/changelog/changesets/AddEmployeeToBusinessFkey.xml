<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.23.xsd">

    <changeSet id="addEmployeeBusinessFKey" author="mykolas">
        <addForeignKeyConstraint
            baseTableName="employee"
            baseColumnNames="business_id"
            constraintName="fk_employee_business"
            referencedTableName="business"
            referencedColumnNames="id" />
    </changeSet>

</databaseChangeLog>
